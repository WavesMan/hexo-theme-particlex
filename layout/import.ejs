<script>
  (function () {
    var endpoint = "https://s3module-vev5d5srwq-du.a.run.app";
    var origFetch = window.fetch;
    window.fetch = function (input, init) {
      try {
        var url = typeof input === "string" ? input : (input && input.url);
        if (typeof url === "string" && url.indexOf("http://127.0.0.1:5001/sitescansense/asia-northeast3/s3module") === 0) {
          var suffixIndex = url.indexOf("/s3module") + 9;
          var suffix = url.substring(suffixIndex);
          input = endpoint + suffix;
        }
      } catch (e) {}
      return origFetch.call(this, input, init);
    };
    document.addEventListener("DOMContentLoaded", function () {
      try {
        if (window.SITESCANSENSE && window.SITESCANSENSE.prototype) {
          window.SITESCANSENSE.prototype.endpoint = endpoint;
        }
      } catch (e) {}
    }, { once: true });
  })();
</script>
